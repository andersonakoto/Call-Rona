<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contact_sea"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CallActivity">


    <ProgressBar
        android:id="@+id/rolling_progress"
        android:layout_width="79dp"
        android:layout_height="62dp"
        android:layout_marginTop="110dp"
        android:layout_marginBottom="19dp"
        app:layout_constraintBottom_toTopOf="@+id/rolling_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/rolling_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="48dp"
        android:text="Rolling..."
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/roulette_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rolling_progress" />

    <ImageView
        android:id="@+id/statistik_img"
        android:layout_width="70dp"
        android:layout_height="62dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="180dp"
        android:src="@drawable/statistik_img"
        app:layout_constraintBottom_toBottomOf="@+id/contact_img"
        app:layout_constraintEnd_toStartOf="@+id/contact_img"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/contact_img"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/contact_img"
        android:layout_width="71dp"
        android:layout_height="67dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:src="@drawable/contact_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/statistik_img"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/call_img"
        android:layout_width="124dp"
        android:layout_height="125dp"
        android:background="@drawable/telephon"
        android:translationZ="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/roulette_img"
        android:layout_width="257dp"
        android:layout_height="208dp"
        android:src="@drawable/roullete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <ImageView
        android:id="@+id/wurfel_img"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:background="@drawable/wurfel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/view_contact"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="71dp"
        android:layout_height="67dp"
        app:layout_constraintBottom_toBottomOf="@+id/contact_img"
        app:layout_constraintEnd_toEndOf="@+id/contact_img"
        app:layout_constraintStart_toStartOf="@+id/contact_img"
        app:layout_constraintTop_toTopOf="@+id/contact_img" />

    <Button
        android:id="@+id/view_statistics"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="70dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="@+id/statistik_img"
        app:layout_constraintEnd_toEndOf="@+id/statistik_img"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/statistik_img"
        app:layout_constraintTop_toTopOf="@+id/statistik_img"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/roll_contacts"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/roulette_img"
        app:layout_constraintEnd_toEndOf="@+id/roulette_img"
        app:layout_constraintStart_toStartOf="@+id/roulette_img"
        app:layout_constraintTop_toTopOf="@+id/roulette_img" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:text="Statistik"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statistik_img" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="197dp"
        android:layout_marginEnd="30dp"
        android:text="Contacts"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/statistik_img"
        app:layout_constraintTop_toBottomOf="@+id/contact_img" />


</androidx.constraintlayout.widget.ConstraintLayout>